#: ../data/bisho.desktop.in.h:1 ../src/bisho-window.c:146
msgid "My Web Accounts"
msgstr "ਮੇਰੇ ਵੈੱਬ ਅਕਾਊਂਟ"

#. Please don't translate "myzone"
#: ../data/bisho.desktop.in.h:3
msgid "myzone & status"
msgstr "myzone & ਹਾਲਤ"

#: ../src/bisho-pane.c:86
msgid "Launch site for more information."
msgstr "ਹੋਰ ਜਾਣਕਾਰੀ ਲਈ ਸਾਈਟ ਚਲਾਓ।"

#: ../src/bisho-pane.c:90
#, c-format
msgid ""
"<small>You'll need an account with %s and an Internet connection to use this "
"web service.</small>"
msgstr ""
"<small>ਤੁਹਾਨੂੰ ਇਹ ਵੈੱਬ ਸਰਵਿਸ ਨੂੰ ਵਰਤਣ ਲਈ %s ਨਾਲ ਅਕਾਊਂਟ ਅਤੇ ਇੰਟਰਨੈੱਟ ਕੁਨੈਕਸ਼ਨ ਵੀ ਚਾਹੀਦਾ ਹੈ।</"
"small>"

#: ../src/bisho-pane.c:195
#, c-format
msgid "Sorry, we can't log in to %s. %s"
msgstr "ਅਫਸੋਸ, %s ਉੱਤੇ ਅਸੀਂ ਲਗਾਇਨ ਨਹੀਂ ਕਰ ਸਕਦੇ। %s"

#: ../src/bisho-pane.c:199
#, c-format
msgid "Sorry, we can't log in to %s"
msgstr "ਅਫਸੋਸ, ਅਸੀਂ %s ਵਿੱਚ ਲਾਗਇਨ ਨਹੀਂ ਕਰ ਸਕਦੇ।"

#: ../src/bisho-pane-facebook.c:329 ../src/bisho-pane-flickr.c:267
#: ../src/bisho-pane-oauth.c:314
msgid "Log me in"
msgstr "ਮੈਨੂੰ ਲਾਗਇਨ ਕਰੋ"

#: ../src/bisho-pane-facebook.c:335 ../src/bisho-pane-flickr.c:273
#: ../src/bisho-pane-oauth.c:320
msgid "Working..."
msgstr "ਜਾਰੀ ਹੈ..."

#. Use the same string in bisho-pane-oauth.c for translations.
#: ../src/bisho-pane-facebook.c:342 ../src/bisho-pane-oauth.c:328
#, c-format
msgid "Once you have logged in to %s, press Continue."
msgstr "ਜਦੋਂ ਤੁਸੀਂ %s ਵਿੱਚ ਲਾਗਇਨ ਕਰ ਗਏ ਤਾਂ ਜਾਰੀ ਰੱਖੋ ਦੱਬੋ।"

#: ../src/bisho-pane-facebook.c:347 ../src/bisho-pane-flickr.c:278
#: ../src/bisho-pane-oauth.c:333 ../src/bisho-pane-oauth.c:350
msgid "Continue"
msgstr "ਜਾਰੀ ਰੱਖੋ"

#: ../src/bisho-pane-facebook.c:352 ../src/bisho-pane-flickr.c:282
#: ../src/bisho-pane-oauth.c:355
msgid "Log in succeeded. You'll see new items in a couple of minutes."
msgstr "ਲਾਗਇਨ ਸਫ਼ਲ ਰਿਹਾ ਹੈ। ਤੁਹਾਡੀ ਕੁਝ ਕੁ ਮਿੰਟਾਂ ਵਿੱਚ ਨਵੀਆਂ ਆਈਟਮਾਂ ਵੇਖੋਗੇ।"

#: ../src/bisho-pane-facebook.c:355 ../src/bisho-pane-flickr.c:285
#: ../src/bisho-pane-oauth.c:357
msgid "Log me out"
msgstr "ਮੈਨੂੰ ਲਾਗ ਆਉਟ ਕਰੋ"

#: ../src/bisho-pane-flickr.c:277
msgid "Once you have logged in to Flickr, press Continue."
msgstr "ਜਦੋਂ ਤੁਸੀਂ ਫਲਿੱਕਰ 'ਚ ਲਾਗਇਨ ਕਰ ਗਏ ਤਾਂ ਜਾਰੀ ਰੱਖੋ ਦੱਬੋ।"

#: ../src/bisho-pane-oauth.c:86
msgid "You don't seem to have a network connection, this won't work."
msgstr "ਤੁਹਾਡੇ ਕੋਲ ਨੈੱਟਵਰਕ ਕੁਨੈਕਸ਼ਨ ਨਹੀਂ ਜਾਪਦਾ ਹੈ, ਇਹ ਕੰਮ ਨਹੀਂ ਕਰੇਗਾ।"

#: ../src/bisho-pane-oauth.c:87
msgid "You could check that the computer's clock is correct."
msgstr "ਤੁਸੀਂ ਚੈੱਕ ਕਰ ਸਕਦੇ ਹੋ ਕਿ ਤੁਹਾਡੇ ਕੰਪਿਊਟਰ ਦੀ ਘੜੀ ਠੀਕ ਹੈ।"

#: ../src/bisho-pane-oauth.c:88
msgid "You could try again."
msgstr "ਤੁਸੀਂ ਫੇਰ ਕੋਸ਼ਿਸ਼ ਕਰ ਸਕਦੇ ਹੋ।"

#: ../src/bisho-pane-oauth.c:345
#, c-format
msgid ""
"Once you have logged in to %s, enter the code they give you and press "
"Continue."
msgstr "ਜਦੋਂ ਤੁਸੀਂ %s ਵਿੱਚ ਲਾਗਇਨ ਕਰ ਗਏ ਤਾਂ ਉਹਨਾਂ ਵਲੋਂ ਦਿੱਤਾ ਕੋਡ ਦਿਓ ਤੇ ਜਾਰੀ ਰੱਖੋ ਦੱਬੋ।"

#: ../src/bisho-pane-oauth.c:399
msgid "Code:"
msgstr "ਕੋਡ:"

#: ../src/bisho-pane-username.c:57
#, c-format
msgid "%s login changed."
msgstr "%s ਲਾਗਇਨ ਬਦਲਿਆ"

#: ../src/bisho-window.c:76 ../src/bisho-window.c:83
msgid "Username:"
msgstr "ਯੂਜ਼ਰ-ਨਾਂ:"

#: ../src/bisho-window.c:85
msgid "Password:"
msgstr "ਪਾਸਵਰਡ:"

#: ../src/bisho-window.c:171
msgid "Quit"
msgstr ""

#. Please don't translate 'myzone'
#: ../src/bisho-window.c:182
msgid ""
"Set up your social networks to see new updates in myzone and the status "
"panel."
msgstr "ਆਪਣਾ ਸ਼ੋਸ਼ਲ ਨੈੱਟਵਰਕ ਸੈੱਟਅੱਪ ਕਰੋ ਤਾਂ ਕਿ ਮੇਰੇ-ਜ਼ੋਨ ਅਤੇ ਹਾਲਤ ਪੈਨਲ ਲਈ ਨਵੇਂ ਅੱਪਡੇਟ ਵੇਖ ਸਕੋ।"

